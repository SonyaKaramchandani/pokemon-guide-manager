{
  "name": "biod.insights.app",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:5001",
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@mdx-js/react": "^1.5.3",
    "@reach/router": "^1.2.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@theme-ui/match-media": "^0.3.1",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.19.0",
    "base-64": "^0.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.8.1",
    "lodash.debounce": "^4.0.8",
    "lodash.orderby": "^4.6.0",
    "lodash.truncate": "^4.4.2",
    "react": "^16.12.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.4",
    "semantic-ui-react": "^0.88.2",
    "shiitake": "^3.0.2",
    "theme-ui": "^0.2.52",
    "utf8": "^3.0.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "eject": "craco eject",
    "test": "react-scripts test --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html",
    "test:ci": "react-scripts test --watchAll=false --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "react-app"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".d.ts",
            ".tsx"
          ],
          "paths": [
            "./src"
          ]
        }
      }
    },
    "rules": {
      "react/jsx-filename-extension": "off",
      "no-underscore-dangle": "warn",
      "prefer-destructuring": "warn",
      "react/prop-types": "off",
      "react/destructuring-assignment": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "no-nested-ternary": "warn"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@craco/craco": "^5.6.2",
    "@semantic-ui-react/craco-less": "^1.2.1",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/chai": "^4.2.10",
    "@types/classnames": "^2.2.9",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.truncate": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@types/theme-ui": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.27",
    "chai": "^4.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "husky": "^4.2.3",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.7",
    "mocha": "^7.1.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prop-types": "15.6.0",
    "semantic-ui-less": "^2.4.1",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
