@model Biod.Surveillance.ViewModels.ArticleDetailsById
@using System.Configuration
@{
    var translationStartDate = Convert.ToDateTime("03/28/2019"); // (MM/DD/YYYY) Date when article translation has started for Foreign articles of Ham Type 3
}

<script>

    $(".scrollbar").css({ "height": $(window).innerHeight() - $(".navbar").innerHeight() - 5 });

    function triggerLoadingMaskOn() {
        $(".overlay").show();
        $(".loading").show();
    }

    function triggerLoadingMaskOff() {
        $(".overlay").hide();
        $(".loading").hide();
    }


/*Unlink from cluster click*/

$("#articleInfo-wrap-toggle").on('click', '.clusterLink', function (event) {

    triggerLoadingMaskOn();
    var clusterID = $(this).attr("id");

    var activated = "";

    $(this).parent().html(function () {
        var html = "";
        if ($(".unlink-fromCluster").is(":visible")) { //Unlink From Cluster activated
            activated = "unlink";
            clusterID = Math.abs(clusterID) * -1; //negative clusterid
            html = "<a id=" + clusterID + " class='relink-toCluster clusterLink'><span>RELINK TO CLUSTER</span></a>";

        } else {  //Relink From Cluster activated
            activated = "relink";
            clusterID = Math.abs(clusterID);
            html = "<a id=" + clusterID + " class='unlink-fromCluster clusterLink'><span>UNLINK FROM CLUSTER</span></a>";
        }

        // Update Article Cluster Id
        var articleID =  @Html.Raw(Json.Encode(Model.ArticleDetails.ArticleID));
        $.ajax({
            url: "/"+"@ConfigurationManager.AppSettings.Get("surveillanceVirtualDirector")"+"/Home/UpdateArticleClusterID?articleID=" + articleID +"&&clusterID= " + clusterID,
            type: "GET",
            success: function () {

                var selectedTypeId = $("#UL-articles").find("li a.active").attr("id");

                if (activated == "unlink") {

                    var articleDataTable = $("#article-grid").find('#exampleGridTest').DataTable();
                    var currentViewedPageNum = articleDataTable.page();

                    $("#viewPlaceHolder").load("/"+"@ConfigurationManager.AppSettings.Get("surveillanceVirtualDirector")"+"/home/GetInitialArticleList?ID=" + selectedTypeId + "&&viewName=_ArticleFilter", function (response, status, xhr) {
                        if (status == "success") {
                            triggerLoadingMaskOff();
                            var table = $("#article-grid").find('#exampleGridTest').DataTable();
                            table.page(currentViewedPageNum).draw('page');
                            $("#viewPlaceHolder-ArticleDetails").hide();
                            $("#viewPlaceHolder").show();
                        }
                    });

                } else {

                    $("#viewPlaceHolder").load("/"+"@ConfigurationManager.AppSettings.Get("surveillanceVirtualDirector")"+"/home/GetInitialArticleList?ID=" + selectedTypeId + "&&viewName=_ArticleFilter", function (response, status, xhr) {
                        if (status == "success") {
                            triggerLoadingMaskOff();
                            var table = $("#article-grid").find('#exampleGridTest').DataTable();
                            table.page(0).draw('page');
                            $("#viewPlaceHolder-ArticleDetails").hide();
                            $("#viewPlaceHolder").show();
                        }
                    });
                }
            }
        });

        return html;
    });
});

/*.....Back Button.....*/
$(".backButton").click(function () {
    if (window.location.search != "") {
        window.location.href = window.location.href.split("?")[0]; //"http://www.newurl.com";
    }
    $("#viewPlaceHolder-ArticleDetails").hide();
    $("#viewPlaceHolder").show();
});

/*.....Expand Button.....*/
$("#article-edit-wrap-toggle").click(function () {
    $header = $(this);
    $("#article-edit-wrap-container").slideToggle(function () {
        $header.html(function () {
            var myhtml = $("#article-edit-wrap-container").is(":visible") ? "<p class='text-right'>Collapse  <a href='#'><span class='glyphicon glyphicon-th'></span></a></p>"
                : "<p class='text-right'>Expand  <a href='#'><span class='glyphicon glyphicon-th'></span></a></p>";
            return myhtml;
        });
    });
});

    //.....Opens Article MetaData window
    $("#openArticleMetadataWindow").click(function () {

        var articleID = @Html.Raw(Json.Encode(Model.ArticleDetails.ArticleID));

        triggerLoadingMaskOn();
        $.get("/"+"@ConfigurationManager.AppSettings.Get("surveillanceVirtualDirector")"+"/Home/GetArticleModalWindow",
            { Id: articleID, hasSimilarArticle: false, isChild: false, parentId: "" })
            .done(function (dialogHTML) {
                triggerLoadingMaskOff();
                $("#articleModalWindow").html(dialogHTML);
                $("#OpenArticleMetadataDialog").trigger("click");
            });
    });




</script>


<div class="scrollbar scrollbar-primary" style="height:578px">
    <div class="force-overflow">

        <div id="articleInfo-wrap-toggle" class="container-fluid">
            <div style="padding-left:10px; padding-right:10px">
                <div class="row">
                    <div class="col-sm-9">
                        <span>
                            <button type="button" class="btn btn-info backButton">
                                <span class="glyphicon glyphicon-chevron-left"></span> Back
                            </button>
                            <br />
                        </span>
                    </div>
                    <br />
                </div>
                <br />
                <div class="row">

                    <div class="col-sm-12">
                        <div>
                            <span><strong>@Html.DisplayFor(m => m.ArticleDetails.ArticleTitle)</strong></span>
                        </div>
                        <div>
                            <span>@Model.ArticleDetails.FeedPublishedDate.ToString("MMMM dd, yyyy") / @Html.DisplayFor(m => m.ArticleDetails.ArticleFeedName) / <span>@Html.DisplayFor(m => m.ArticleDetails.ArticleOriginalLanguage)</span> / <span><a href="@Html.DisplayFor(m => m.ArticleDetails.ArticleURL)" target="_blank">@Html.DisplayFor(m => m.ArticleDetails.ArticleURL)</a></span></span>
                        </div>
                        <div id="currentArticleUsers-container" style="display:none">
                            <div id="currentArticleUsers">
                                <span>Currently being edited by &nbsp;</span>
                                <span id="articleId" style="display:none">@Html.DisplayFor(m => m.ArticleDetails.ArticleID)</span>
                            </div>
                        </div>
                        <div>
                            @if (Model.ArticleDetails.IsCompleted.HasValue && Model.ArticleDetails.IsCompleted == true)
                            {

                                @Html.Display("");
                            }
                            else
                            {
                                if (Model.ArticleDetails.IsChildArticle == true)
                                {
                                    <div>

                                        <span>Similar (/duplicate) article to </span><span>@Model.ArticleDetails.ParentArticleTitle</span>
                                    </div>
                                }
                                else
                                {
                                    <div id="unprocessed-status"><span>Unprocessed article</span></div>
                                }

                            }
                        </div>
                        <div style="padding-bottom:5px">
                            @*<button type="button" id="openArticleMetadataWindow" class="btn btn-info btn-sm" style="float:left">EDIT METADATA</button>*@
                            <br />
                            <div style="display: inline-block">

                                @if (Model.ArticleDetails.IsChildArticle == true) //clusterID is never NULL and always positive when it has child article
                                {
                                    @Html.Display("");
                                }
                                else
                                {
                                    <a id="openArticleMetadataWindow" style="text-decoration:none; cursor:pointer">
                                        <div>
                                            <span class='glyphicon glyphicon-pencil'></span><span style='padding-right:3px'></span>
                                            <span><strong>EDIT METADATA</strong></span>
                                        </div>
                                    </a>
                                }


                            </div>
                            <div style="display: inline-block; float:right">

                                @if (Model.ArticleDetails.HasSimilarArticle == true) //clusterID is never NULL and always positive when it has child article
                                {
                                    //....has similar or duplicate article
                                    <div class="text-right">
                                        <a id="@Model.ArticleDetails.SimilarClusterId" class="unlink-fromCluster clusterLink"><span>UNLINK FROM CLUSTER</span></a>
                                    </div>

                                }
                                else
                                {
                                    //....No similar/duplicate article  (possible cluster values ---- null, positive or negative)

                                    if (Model.ArticleDetails.SimilarClusterId > 0 || Model.ArticleDetails.SimilarClusterId == null)
                                    {
                                        //....positive cluster ID or NULL
                                        <div class="text-right">
                                            <a style="color:grey; text-decoration:none"><span>UNLINK FROM CLUSTER</span></a>
                                        </div>

                                    }
                                    else
                                    {
                                        //...Negative cluster ID
                                        <div class="text-right">
                                            <a id="@Model.ArticleDetails.SimilarClusterId" class="relink-toCluster clusterLink"><span>RELINK TO CLUSTER</span></a>
                                        </div>
                                    }
                                }

                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
        @{
            if (Model.ArticleDetails.ArticleOriginalLanguage != "en" && Model.ArticleDetails.ArticleHamTypeId == 3 && Model.ArticleDetails.FeedPublishedDate >= translationStartDate)
            {
                <div style="padding-top:20px;">
                    <span style="font-family:Arial; font-size:14px">Translated from <b>@Model.ArticleDetails.ArticleTranslatedFrom</b>:</span>
                </div>
            }
        }

        <div id="article-contents" class="container-fluid">

            <!--  Container for article contents retrieved from the URL  -->
            <div class="row">
                <div class="col-sm-12">
                    @if (Model.ArticleDetails.IsChildArticle == true)
                    {
                        @Html.Display("");
                    }
                    else
                    {
                        //Parent articles
                        if (String.IsNullOrEmpty(Model.ArticleDetails.ArticleBody))
                        {
                            <p class="text-center">
                                <span>Article URL</span><br />
                                <span><a href="@Html.DisplayFor(m => m.ArticleDetails.ArticleURL)">@Html.DisplayFor(m => m.ArticleDetails.ArticleURL)</a></span> <br>
                            </p>
                        }
                        else
                        {

                            if (Model.ArticleDetails.ArticleOriginalLanguage == "en" || (Model.ArticleDetails.ArticleOriginalLanguage != "en" && Model.ArticleDetails.ArticleHamTypeId == 3 && Model.ArticleDetails.FeedPublishedDate >= translationStartDate))
                            {
                                <p class="text-left">
                                    <pre>@Html.DisplayFor(m => m.ArticleDetails.ArticleBody)</pre>
                                </p>
                            }
                            else
                            {
                                <p class="text-center">
                                    <span>Article URL</span><br />
                                    <span><a href="@Html.DisplayFor(m => m.ArticleDetails.ArticleURL)">@Html.DisplayFor(m => m.ArticleDetails.ArticleURL)</a></span><br>
                                </p>
                            }
                        }
                    }
                </div>
            </div>
            <br />
        </div>
        @{
            if (Model.ArticleDetails.ArticleOriginalLanguage != "en" && Model.ArticleDetails.ArticleHamTypeId == 3 && Model.ArticleDetails.FeedPublishedDate >= translationStartDate)
            {
                <div class="googletranslate-logo">
                    <a href="https://translate.google.com/" target="_blank">
                        <svg width="131" height="17" viewBox="0 0 131 17" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect x="0.692871" width="130.279" height="17" fill="url(#pattern0)" />
                            <defs>
                                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0" transform="translate(0 -0.00252334) scale(0.00273224 0.0209385)" />
                                </pattern>
                                <image id="image0" width="366" height="48" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </a>
                </div>
            }
        }

    </div>
</div>